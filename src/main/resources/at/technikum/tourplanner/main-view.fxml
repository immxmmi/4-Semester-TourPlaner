                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>


                            <children>
                                <Label text="Tour ID" />
                                <TextField GridPane.columnIndex="1" fx:id="inputTourID" />
                                <Label text="Name" GridPane.rowIndex="1" />
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="inputName"/>
                                <Label text="City Name A (form)" GridPane.rowIndex="2"/>
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="inputFrom" />
                            </children>



                        </GridPane>
                        <GridPane GridPane.rowIndex="1">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="117.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="197.0" minWidth="10.0" prefWidth="183.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label text="City Name B (to)" />
                                <TextField prefHeight="25.0" prefWidth="128.0" GridPane.columnIndex="1" fx:id="inputTo"/>
                                <Label text="Image Name" GridPane.rowIndex="1" />
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="inputImage" />
                                <Label text="transporter" GridPane.rowIndex="2" />
                                <TextField text="Walk" GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="inputTransporter"/>

                            </children>
                        </GridPane>
                        <GridPane GridPane.rowIndex="2">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="117.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="202.0" minWidth="10.0" prefWidth="183.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>


                            <children>
                                <Label text="distance" />
                                <TextField prefHeight="25.0" prefWidth="161.0" GridPane.columnIndex="1" fx:id="inputDistance"/>
                                <Label text="description" GridPane.rowIndex="1" />
                                <TextField prefHeight="25.0" prefWidth="156.0" GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="inputDescription" />
                                <Label text="time" GridPane.rowIndex="2" />
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="inputTime" />
                            </children>

                        </GridPane>
                    </children>
                </GridPane>
